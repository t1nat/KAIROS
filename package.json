{
  "name": "kairos",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "pnpm run lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate --config=config/drizzle.config.ts",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate --config=config/drizzle.config.ts",
    "db:push": "dotenv -e .env -- drizzle-kit push --config=config/drizzle.config.ts",
    "db:studio": "dotenv -e .env -- drizzle-kit studio --config=config/drizzle.config.ts",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "postinstall": "npm run db:generate"
  },
